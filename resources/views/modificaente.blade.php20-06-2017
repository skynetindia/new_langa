<script>
function punto() {
	$j('#prova').val($('#pac-input').val());
}
$j('.ciao').on("click", function() {
	$(this).children()[0].click();
});
$j('#btnSubmiTop').on("click", function() {
	$j("#btnSubmitEnti").click();
