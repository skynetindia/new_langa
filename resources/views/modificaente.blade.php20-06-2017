	setTimeout(function() {
		var cellulari = ["<?php
			for($i=1;$i<count($users);$i++) {
				if($i == count($users) - 1)
					echo $users[$i]->cellulare . "\"";
				else
					echo $users[$i]->cellulare . "\",\"";
			}
		?>];
		var nomi = ["<?php
			for($i=1;$i<count($users);$i++) {
				if($i == count($users) - 1)
					echo $users[$i]->name . "\"";
				else
					echo $users[$i]->name . "\",\"";
			}
		?>];
			var k;
			var nome = $j("#responsabilelanga option:selected" ).text();
			for(var i = 0; i < <?php echo count($users)-1;?>;i++) {
				if(nomi[i] == nome) {
					k = i;
					break;
				}
			}
			$j('#telefonoresponsabile').val(cellulari[k]);
	}, 100);
	function trovaTelefono() {
		var cellulari = ["<?php
			for($i=1;$i<count($users);$i++) {
				if($i == count($users) - 1)
					echo $users[$i]->cellulare . "\"";
				else
					echo $users[$i]->cellulare . "\",\"";
			}
		?>];
		var nomi = ["<?php
			for($i=1;$i<count($users);$i++) {
				if($i == count($users) - 1)
					echo $users[$i]->name . "\"";
				else
					echo $users[$i]->name . "\",\"";
			}
		?>];
			var k;
			var nome = $j("#responsabilelanga option:selected" ).val();
			for(var i = 0; i < <?php echo count($users)-1;?>;i++) {
				if(nomi[i] == nome) {
					k = i;
					break;
				}
			}
			$j('#telefonoresponsabile').val(cellulari[k]);
	}
